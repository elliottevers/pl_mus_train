{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": false,
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "typeRoots": [ "node_modules/@types" ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "src"
  ]
}